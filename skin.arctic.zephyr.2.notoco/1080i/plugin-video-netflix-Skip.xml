<?xml version="1.0" encoding="UTF-8"?>
<window>
    <defaultcontrol always="true">6012</defaultcontrol>
    <onload>Dialog.Close(fullscreeninfo,true)</onload>
    <onload>Dialog.Close(videoosd,true)</onload>
    <onunload>Dialog.Close(fullscreeninfo,true)</onunload>
    <onunload>Dialog.Close(videoosd,true)</onunload>
    <controls>
        <control type="group">
            <left>view_pad</left>
            <right>15</right>
            <bottom>15</bottom>
            <height>60</height>
            <animation type="WindowOpen" reversible="false">
                <effect type="fade" start="0" end="100" time="400" />
                <effect type="slide" start="0,50" end="0,0" time="400" />
            </animation>
            <animation type="WindowClose" reversible="false">
                <effect type="fade" start="100" end="0" time="400" />
                <effect type="slide" start="0,0" end="0,50" time="400" />
            </animation>
            <control type="button" id="6012">
                <description>Skip intro</description>
                <height>56</height>
                <font>font20_title</font>
                <textoffsetx>55</textoffsetx>
                <textcolor>ddffffff</textcolor>
                <aligny>center</aligny>
                <align>center</align>
                <width>350</width>
                <texturefocus colordiffuse="panel_bg" border="10">diffuse/box.png</texturefocus>
                <texturenofocus colordiffuse="panel_bg" border="10">diffuse/box.png</texturenofocus>
            </control>
        </control>
    </controls>
</window>
