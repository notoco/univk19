<?xml version="1.0" encoding="UTF-8"?>
<window>
  <coordinates>
    <top>0</top>
    <left>0</left>
    <width>1920</width>
    <height>1080</height>
  </coordinates>

  <!-- dimmed background -->
  <controls>
    <control type="image">
      <width>1920</width>
      <height>1080</height>
      <texture background="true" colordiffuse="BF000000">white.png</texture>
    </control>

    <control type="group">
        <width>400</width>
        <control type="image">
          <texture background="true" colordiffuse="FF000000">white.png</texture>
        </control>
        <control type="image">
          <width>2</width>
          <right>-1</right>
          <texture background="true" colordiffuse="FF999999">white.png</texture>
        </control>
    </control>
  </controls>

</window><!--
 vim:sw=2:ts=2:sts=2:
-->